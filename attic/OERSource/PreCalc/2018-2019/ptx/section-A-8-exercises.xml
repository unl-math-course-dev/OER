<exercisegroup cols="2">
    <introduction><p>Factor completely.
    </p></introduction>

    <exercise number="1"><statement>
        <p><m>x^2+5x+6 </m></p>
        </statement>
        <answer><p><m>(x+2)(x+3) </m></p></answer>
    </exercise>

    <exercise number="2"><statement>
        <p><m>x^2+5x+4</m></p>
        </statement>
    </exercise>

    <exercise number="3"><statement>
        <p><m>y^2-7y+12</m></p>
        </statement>
        <answer><p>(y-3)(y-4) </p></answer>
    </exercise>

    <exercise number="4"><statement>
        <p><m>y^2-7y+10</m></p>
        </statement>
    </exercise>

    <exercise number="5"><statement>
        <p><m>x^2-6-x</m></p>
        </statement>
        <answer><p><m>(x-3)(x+2) </m></p></answer>
    </exercise>

    <exercise number="6"><statement>
        <p><m>x^2-15-2x</m></p>
        </statement>
    </exercise>

    <exercise number="7"><statement>
